<%= form_for@course do |f|%>
<%= f.label 'Name', :class => 'label'%>
<%= f.text_field :name, placeholder: 'Name', :class => "form-control"%><br>
<%= f.label 'Hours', :class => 'label'%>
<%= f.number_field :total_hours, placeholder: 'Total hours', :class => "form-control"%><br>
<%= f.submit :class=>"btn" %>
<%end%>
