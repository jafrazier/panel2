<%= form_for @instructor do |f| %>
<%= f.label 'First name' %>
<%= f.text_field :first_name %><br>
<%= f.label 'Last name' %>
<%= f.text_field :last_name %><br>
<%= f.label 'Age' %>
<%= f.number_field :age %><br>
<%= f.label 'Education'%>
<%= f.text_field :education%><br>
<%= f.label 'Salary' %>
<%= f.number_field :salary %><br>
<%= f.label 'Email'%>
<%= f.email_field :email%><br>
<%= f.label 'Password'%>
<%= f.text_field :email%><br>
<%= f.submit %>
<% end %>
