<%= form_for @cohort do |f| %>
<%= f.label 'Name'%>
<%= f.text_field :name %><br>
<%= f.label 'Start date' %>
<%= f.date_field :start_date %><br>
<%= f.label 'End date' %>
<%= f.date_field :end_date %><br>
<%= f.label 'Course'%>
<%= f.collection_select :course_id, @courses, :id, :name, {include_blank: true} %><br>
<%= f.label 'Instructor'%>
<%= f.collection_select :instructor_id, @instructors, :id, :full_name , {include_blank: true} %><br>
<%= f.label 'Student'%>
<%= f.collection_select :student_id, @students , :id, :full_name , {include_blank: true} %><br>
<%= f.label 'Student'%>
<%= f.collection_select :student_id, @students , :id, :full_name , {include_blank: true} %><br>

<%= f.submit %>
<% end %>
