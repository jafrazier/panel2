<%= form_for @student do |f| %>
<%= f.label 'First name' %>
<%= f.text_field :first_name %><br>
<%= f.label 'Last name' %>
<%= f.text_field :last_name %><br>
<%= f.label 'Age' %>
<%= f.number_field :age %><br>
<%= f.label 'Education'%>
<%= f.text_field :education%><br>
<%= f.collection_select :cohort_id, Cohort.all , :id, :name , {include_blank: true} %><br>
<%= f.submit %>
<% end %>
